{
  "name": "devextreme-vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier . --write",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "devextreme": "23.2.3",
    "devextreme-vue": "23.2.3",
    "firebase": "^10.9.0",
    "sass": "^1.34.1",
    "vue": "^3.2.13",
    "vue-router": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "devextreme-cli": "1.6.3",
    "devextreme-themebuilder": "23.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass-loader": "^10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
